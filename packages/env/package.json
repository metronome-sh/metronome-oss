    "vite-env-only": "^2.2.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@epic-web/remember": "^1.0.2",
    "dotenv": "^16.4.5",
    "ts-invariant": "^0.10.3"
  },
  "sideEffects": false
