{
  "name": "@metronome/db",
  "version": "4.0.0",
  "author": "Erick Tamayo",
  "license": "FSL-1.0-MIT",
  "exports": {
    "import": "./dist/index.server.js",
    "types": "./dist/index.d.ts"
  },
  "type": "module",
  "scripts": {
    "build": "concurrently --raw pnpm:build:*",
    "build:esbuild": "node --experimental-import-meta-resolve scripts/esbuild.mjs",
    "build:typings": "tsc",
    "dev": "concurrently --raw pnpm:dev:*",
    "dev:typings": "tsc --watch",
    "dev:esbuild": "node --experimental-import-meta-resolve scripts/esbuild.watch.mjs",
    "dk": "drizzle-kit",
    "migrate:up": "tsx ./drizzle/migrate.mts",
    "geoip:download": "NODE_ENV=production tsx ./cli/geoip.mts",
    "test": "vitest run",
    "configure": "pnpm migrate:up && pnpm clickhouse:up && pnpm geoip:download ",
    "drizzle:up": "tsx ./drizzle/migrate.mts",
    "clickhouse:create": "migrate -c ./clickhouse --template-file ./template.cjs create",
    "clickhouse:up": "migrate -c ./clickhouse --store=./store.cjs up",
    "clickhouse:down": "migrate -c ./clickhouse --store=./store.cjs down",
    "user:reset-password": "node ./scripts/reset-password.mjs"
  },
  "keywords": [],
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/lodash-es": "^4.17.12",
    "@types/pg": "^8.11.4",
    "@types/tar": "^6.1.12",
    "@types/tldjs": "^2.3.4",
    "@types/ua-parser-js": "^0.7.39",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.14",
    "esbuild": "^0.19.12",
    "i": "^0.3.7",
    "typescript": "^5.4.4",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.550.0",
    "@clickhouse/client": "^0.2.10",
    "@epic-web/remember": "^1.0.2",
    "@js-temporal/polyfill": "^0.4.4",
    "@maxmind/geoip2-node": "^4.2.0",
    "@metronome/cache": "workspace:*",
    "@metronome/env": "workspace:*",
    "@metronome/queues": "workspace:*",
    "adm-zip": "^0.5.12",
    "argon2": "^0.40.1",
    "cheerio": "1.0.0-rc.12",
    "device-detector-js": "^3.0.3",
    "drizzle-orm": "^0.29.5",
    "execa": "^8.0.1",
    "ioredis": "^5.3.2",
    "lodash-es": "^4.17.21",
    "migrate": "^2.1.0",
    "nanoid": "^3.3.7",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.5",
    "postgres": "^3.4.4",
    "rxjs": "^7.8.1",
    "shiki": "^1.2.4",
    "slugify": "^1.6.6",
    "source-map": "^0.7.4",
    "stacktrace-parser": "^0.1.10",
    "tar": "^6.2.1",
    "tldjs": "^2.3.1",
    "ts-invariant": "^0.10.3",
    "tsx": "^3.14.0",
    "type-fest": "^4.15.0",
    "ua-parser-js": "^1.0.37",
    "web-vitals": "^3.5.2",
    "zod": "^3.22.4"
  },
  "sideEffects": false
}
